#include <stdio.h>

int main()
{
    int n,a=0,b=1,flag;
     printf("Enter a number: ");
	 scanf("%d",&n);
     if(n==a || n==b)
     {
         printf("Fibonacci number");
     }
     else
     {
         flag=a+b;
         while(flag<n)
         {
             a=b;
             b=flag;
             flag=a+b;
         }
         if(flag==n)
             printf("Yes");
         else
             printf("No");
     }
    return 0;
}
Input:
5
Output:
Yes
